{
    "env": {
        "node": true,
        "es6": true,
        "jest": true
    },
    "root": true,
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "ecmaFeatures": {
            "legacyDecorators": true
        }
    },
    "parser": "babel-eslint",
    "plugins": ["babel"],
    "extends": ["eslint:recommended", "plugin:prettier/recommended", "plugin:react/recommended"],
    "rules": {
        "no-console": 1
    }
}
